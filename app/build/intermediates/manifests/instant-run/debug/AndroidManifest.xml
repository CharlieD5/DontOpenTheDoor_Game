<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alexzh.multiplethemesapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Don&apos;t Open The Door"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme.NoActionBar" >
        <activity android:name="com.charlyan.dontopenthedoor.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.charlyan.dontopenthedoor.ThemesActivity" >
        </activity>
        <activity android:name="com.charlyan.dontopenthedoor.ShareActivity" >
        </activity>
        <activity android:name="com.charlyan.dontopenthedoor.PlayHard" >
        </activity>
        <activity android:name="com.charlyan.dontopenthedoor.PlayEasy" >
        </activity>
        <activity android:name="com.charlyan.dontopenthedoor.PlayMedium" >
        </activity>
        <activity android:name="com.charlyan.dontopenthedoor.DifficultyActivity" >
        </activity>
        <activity android:name="com.charlyan.dontopenthedoor.ScoreActivity" >
        </activity>

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.alexzh.multiplethemesapp.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>