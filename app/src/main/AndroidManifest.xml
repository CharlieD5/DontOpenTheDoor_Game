<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.charlyan.dontopenthedoor">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Don&apos;t Open The Door"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ThemesActivity">
        </activity>
        <activity android:name=".ShareActivity">
        </activity>
        <activity android:name=".PlayHard">
        </activity>
        <activity android:name=".PlayEasy">
        </activity>
        <activity android:name=".PlayMedium">
        </activity>
        <activity android:name=".DifficultyActivity">
        </activity>
        <activity android:name=".ScoreActivity">
        </activity>
    </application>

</manifest>